<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="nav" fx:id="rootPane" prefHeight="60.0" stylesheets="@assets/css/application.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.NavigationViewController">
	<children>
		<JFXButton fx:id="navProfileBtn" onAction="#navProfileBtnOnAction" ripplerFill="#ed4a571a" text="" textAlignment="CENTER" textOverrun="CLIP" GridPane.hgrow="ALWAYS">
			<styleClass>
				<String fx:value="fontAwesome" />
				<String fx:value="profileBtn" />
			</styleClass>
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</JFXButton>
		<JFXButton fx:id="navPlayBtn" onAction="#navPlayBtnOnAction" ripplerFill="#ed4a571a" styleClass="fontAwesome" text="" textAlignment="CENTER" textOverrun="CLIP" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</JFXButton>
		<JFXButton fx:id="navFriendsBtn" onAction="#navFriendsBtnOnAction" ripplerFill="#ed4a571a" styleClass="fontAwesome" text="" textAlignment="CENTER" textOverrun="CLIP" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</JFXButton>
		<JFXButton fx:id="navRatingsBtn" onAction="#navRatingsBtnOnAction" ripplerFill="#ed4a571a" styleClass="fontAwesome" text="" textAlignment="CENTER" textOverrun="CLIP" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</JFXButton>
		<JFXButton fx:id="navNotiBtn" onAction="#navNotiBtnOnAction" ripplerFill="#ed4a571a" styleClass="fontAwesome" text="" textAlignment="CENTER" textOverrun="CLIP" GridPane.columnIndex="4" GridPane.hgrow="ALWAYS">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</JFXButton>
		<JFXButton fx:id="navLogoutBtn" onAction="#navLogoutBtnOnAction" ripplerFill="#ed4a571a" text="" textAlignment="CENTER" textOverrun="CLIP" GridPane.columnIndex="5" GridPane.hgrow="ALWAYS">
			<styleClass>
				<String fx:value="fontAwesome" />
				<String fx:value="logoutBtn" />
			</styleClass>
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</JFXButton>
	</children>
	<columnConstraints>
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" percentWidth="100.0" />
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints valignment="CENTER" />
	</rowConstraints>
</GridPane>
