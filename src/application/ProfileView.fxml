<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<StackPane id="profile" fx:id="stackPaneRoot" maxHeight="1080.0"
	maxWidth="1920.0" prefHeight="900.0" prefWidth="1600.0"
	stylesheets="@assets/css/application.css" xmlns="http://javafx.com/javafx/8.0.141"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ProfileViewController">
	<GridPane id="profile" alignment="CENTER" hgap="25.0" vgap="25.0">
		<children>
			<Pane styleClass="card" GridPane.halignment="CENTER"
				GridPane.valignment="BOTTOM">
				<GridPane.margin>
					<Insets top="140.0" />
				</GridPane.margin>
				<opaqueInsets>
					<Insets />
				</opaqueInsets>
				<padding>
					<Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
				</padding>
			</Pane>
			<GridPane fx:id="profileGridPane" alignment="CENTER" vgap="15.0">
				<columnConstraints>
					<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
						minWidth="10.0" />
				</columnConstraints>
				<rowConstraints>
					<RowConstraints fillHeight="false" minHeight="10.0"
						valignment="CENTER" vgrow="ALWAYS" />
					<RowConstraints minHeight="10.0" valignment="CENTER"
						vgrow="NEVER" />
				</rowConstraints>
				<children>
					<Circle fx:id="dpCircle" fill="DODGERBLUE" radius="100.0"
						stroke="BLACK" strokeType="INSIDE" styleClass="dp">
						<GridPane.margin>
							<Insets />
						</GridPane.margin>
					</Circle>
					<VBox id="basicInfo" alignment="CENTER" GridPane.rowIndex="1">
						<padding>
							<Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
						</padding>
						<children>
							<GridPane alignment="CENTER" vgap="5.0" VBox.vgrow="NEVER">
								<columnConstraints>
									<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" />
								</columnConstraints>
								<rowConstraints>
									<RowConstraints valignment="CENTER" vgrow="NEVER" />
									<RowConstraints valignment="CENTER" vgrow="NEVER" />
									<RowConstraints maxHeight="-Infinity" minHeight="-Infinity"
										prefHeight="5.0" valignment="CENTER" vgrow="NEVER" />
									<RowConstraints valignment="CENTER" vgrow="NEVER" />
									<RowConstraints valignment="CENTER" vgrow="NEVER" />
									<RowConstraints maxHeight="-Infinity" minHeight="-Infinity"
										prefHeight="5.0" valignment="CENTER" vgrow="NEVER" />
									<RowConstraints valignment="CENTER" vgrow="NEVER" />
									<RowConstraints valignment="CENTER" vgrow="NEVER" />
								</rowConstraints>
								<children>
									<Text fx:id="nameTxt" fill="#434343" strokeType="OUTSIDE"
										strokeWidth="0.0" styleClass="h1" text="Name" textAlignment="CENTER" />
									<Text fx:id="heightWeightTxt" fill="#434343" strokeType="OUTSIDE"
										strokeWidth="0.0" styleClass="small" text="Height | Weight"
										GridPane.rowIndex="1">
										<font>
											<Font name="Roboto" size="12.0" />
										</font>
									</Text>
									<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h2"
										text="Player Rating" textAlignment="CENTER" GridPane.rowIndex="3" />
									<Text fx:id="ratingTxt" strokeType="OUTSIDE" strokeWidth="0.0"
										styleClass="ratingStars" text="" GridPane.rowIndex="4" />
									<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h2"
										text="Match Attendance" GridPane.rowIndex="6" />
									<Text fx:id="matchNoTxt" strokeType="OUTSIDE" strokeWidth="0.0"
										styleClass="para" text="0 / 0" GridPane.rowIndex="7" />
								</children>
							</GridPane>
						</children>
					</VBox>
				</children>
				<padding>
					<Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
				</padding>
			</GridPane>
			<GridPane hgap="25.0" vgap="25.0" GridPane.columnIndex="1">
				<columnConstraints>
					<ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
				</columnConstraints>
				<rowConstraints>
					<RowConstraints fillHeight="false" prefHeight="115.0"
						valignment="BOTTOM" vgrow="NEVER" />
					<RowConstraints valignment="CENTER" vgrow="ALWAYS" />
				</rowConstraints>
				<children>
					<FlowPane fx:id="buttonsFlowPane" alignment="CENTER_RIGHT">
						<children>
							<JFXButton fx:id="bckBtn" onAction="#bckBtnOnAction"
								text="" visible="false">
								<styleClass>
									<String fx:value="fontAwesome" />
									<String fx:value="square" />
									<String fx:value="danger" />
								</styleClass>
								<FlowPane.margin>
									<Insets right="25.0" />
								</FlowPane.margin>
								<cursor>
									<Cursor fx:constant="HAND" />
								</cursor>
							</JFXButton>
							<JFXButton fx:id="actionBtn" buttonType="RAISED"
								onAction="#actionBtnOnAction" ripplerFill="#ffffff4d"
								styleClass="success" text="EDIT PROFILE">
								<cursor>
									<Cursor fx:constant="HAND" />
								</cursor>
							</JFXButton>
						</children>
					</FlowPane>
					<Pane prefWidth="600.0" styleClass="card" GridPane.rowIndex="1">
						<GridPane.margin>
							<Insets />
						</GridPane.margin>
					</Pane>
					<GridPane hgap="5.0" vgap="15.0" GridPane.rowIndex="1">
						<columnConstraints>
							<ColumnConstraints hgrow="NEVER" minWidth="10.0" />
							<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
						</columnConstraints>
						<rowConstraints>
							<RowConstraints minHeight="10.0" vgrow="NEVER" />
							<RowConstraints minHeight="10.0" valignment="TOP"
								vgrow="SOMETIMES" />
						</rowConstraints>
						<children>
							<Text strokeType="OUTSIDE" strokeWidth="0.0" text="">
								<styleClass>
									<String fx:value="fontAwesome" />
									<String fx:value="h0" />
								</styleClass>
							</Text>
							<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h0"
								text="INTRODUCTION" GridPane.columnIndex="1" />
							<TextFlow GridPane.columnSpan="2147483647"
								GridPane.rowIndex="1">
								<children>
									<Text fx:id="introTxt" strokeType="OUTSIDE" strokeWidth="0.0"
										styleClass="para" text="Introduce yourself to the world..." />
								</children>
							</TextFlow>
						</children>
						<padding>
							<Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
						</padding>
					</GridPane>
				</children>
			</GridPane>
			<Pane styleClass="card" GridPane.halignment="CENTER"
				GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"
				GridPane.vgrow="ALWAYS" />
			<GridPane alignment="CENTER" vgap="15.0" GridPane.rowIndex="1">
				<columnConstraints>
					<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" />
				</columnConstraints>
				<rowConstraints>
					<RowConstraints vgrow="NEVER" />
					<RowConstraints minHeight="15.0" valignment="TOP"
						vgrow="NEVER" />
					<RowConstraints vgrow="SOMETIMES" />
					<RowConstraints minHeight="10.0" vgrow="NEVER" />
					<RowConstraints minHeight="15.0" vgrow="SOMETIMES" />
				</rowConstraints>
				<children>
					<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1"
						text="Favourite Sport" textAlignment="CENTER" />
					<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1"
						text="Interested Sports" GridPane.rowIndex="3" />
					<TextFlow fx:id="favSportTxtFlow" textAlignment="CENTER"
						GridPane.halignment="CENTER" GridPane.rowIndex="1">
						<children>
							<Label fx:id="favSportChip" styleClass="sportChip" text="None" />
						</children>
					</TextFlow>
					<TextFlow fx:id="intSportsTxtFlow" textAlignment="CENTER"
						GridPane.rowIndex="4" GridPane.vgrow="NEVER">
						<children>
							<Label fx:id="intSportChip" styleClass="sportChip" text="None" />
						</children>
					</TextFlow>
				</children>
				<padding>
					<Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
				</padding>
			</GridPane>
			<Pane styleClass="card" GridPane.columnIndex="1"
				GridPane.rowIndex="1" />
			<GridPane fx:id="commentGridPane" hgap="5.0" vgap="15.0"
				GridPane.columnIndex="1" GridPane.rowIndex="1">
				<columnConstraints>
					<ColumnConstraints hgrow="NEVER" minWidth="10.0" />
					<ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
					<ColumnConstraints halignment="LEFT" hgrow="NEVER"
						minWidth="10.0" />
				</columnConstraints>
				<rowConstraints>
					<RowConstraints minHeight="10.0" vgrow="NEVER" />
					<RowConstraints minHeight="10.0" valignment="CENTER"
						vgrow="SOMETIMES" />
				</rowConstraints>
				<children>
					<Text strokeType="OUTSIDE" strokeWidth="0.0" text="">
						<styleClass>
							<String fx:value="fontAwesome" />
							<String fx:value="h0" />
						</styleClass>
					</Text>
					<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h0"
						text="COMMENTS" GridPane.columnIndex="1" />
					<FlowPane alignment="CENTER_RIGHT" prefWrapLength="70.0"
						styleClass="comBtnFlowPane" GridPane.columnIndex="2">
						<children>
							<JFXButton fx:id="prevComBtn" onAction="#prevComBtnOnAction"
								ripplerFill="#ffffff4d" text="">
								<styleClass>
									<String fx:value="fontAwesome" />
									<String fx:value="inactive" />
									<String fx:value="square" />
									<String fx:value="left" />
								</styleClass>
								<cursor>
									<Cursor fx:constant="HAND" />
								</cursor>
							</JFXButton>
							<JFXButton fx:id="nxtComBtn" onAction="#nxtComBtnOnAction"
								ripplerFill="#ffffff4d" text="">
								<styleClass>
									<String fx:value="fontAwesome" />
									<String fx:value="inactive" />
									<String fx:value="square" />
									<String fx:value="right" />
								</styleClass>
								<cursor>
									<Cursor fx:constant="HAND" />
								</cursor>
							</JFXButton>
						</children>
					</FlowPane>
					<VBox fx:id="commContentVBox" alignment="CENTER" styleClass="commContent"
						GridPane.columnSpan="2147483647" GridPane.rowIndex="1">
						<children>
							<ImageView fitHeight="150.0" fitWidth="150.0"
								pickOnBounds="true" preserveRatio="true">
								<image>
									<Image url="@assets/img/empty_comments.png" />
								</image>
							</ImageView>
							<Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="h1"
								text="Whoops! You do not have any comments..." />
						</children>
					</VBox>
				</children>
				<GridPane.margin>
					<Insets />
				</GridPane.margin>
				<padding>
					<Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
				</padding>
			</GridPane>
		</children>
		<columnConstraints>
			<ColumnConstraints halignment="CENTER" hgrow="NEVER"
				prefWidth="150.0" />
			<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" />
		</columnConstraints>
		<rowConstraints>
			<RowConstraints valignment="CENTER" vgrow="NEVER" />
			<RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
		</rowConstraints>
		<padding>
			<Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
		</padding>
	</GridPane>

</StackPane>
